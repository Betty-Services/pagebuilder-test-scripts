{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "jasmine": true
    },
    "rules": {
      "no-restricted-globals": 0,
      "prettier/prettier": [
        "error",
        { "singleQuote": true, "trailingComma": "all" }
      ]
    }
  }
  